{
    "expo": {
        "name": "MarvelApp",
        "icon": "./assets/icon.png",
        "version": "1.0.0",
        "slug": "MarvelApp",
        "privacy": "public",
        "sdkVersion": "35.0.0",
        "platforms": ["ios", "android", "web"],
        "orientation": "portrait",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.franpastor.marvelAppTest"
        },
        "android": {
            "package": "com.franpastor.marvelAppTest"
        }
    }
}
